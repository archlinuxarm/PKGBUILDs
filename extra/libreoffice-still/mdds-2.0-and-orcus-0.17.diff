-+
-+    // [#x0300-#x036F]
-+    // 0x0300 = 0xcc 0x80 utf-8, 0x36f = 0xcd 0xaf utf-8
-+    if (!only_start_name && first == 0xcc)
-+        return 2;
-+    if (!only_start_name && first == 0xcd && second <= 0xaf)
