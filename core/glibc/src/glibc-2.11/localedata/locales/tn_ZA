% The affirmative response - ee
% "^[yYeE]"
yesexpr     "<U005E><U005B><U0079><U0059><U0065><U0045><U005D>"

% The negative response - nyaa
% "^[nN]"
noexpr      "<U005E><U005B><U006E><U004E><U005D>"
END LC_MESSAGES

