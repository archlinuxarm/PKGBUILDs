%%%%%%%%%%%%%
LC_CTYPE
copy "i18n"

% Oriya uses the alternate digits U+0B66..U+0B6F
outdigit <U0B66>..<U0B6F>

% This is used in the scanf family of functions to read Oriya numbers
% using "%Id" and such.
map to_inpunct; /
  (<U0030>,<U0B66>); /
  (<U0031>,<U0B67>); /
  (<U0032>,<U0B68>); /
  (<U0033>,<U0B69>); /
  (<U0034>,<U0B6A>); /
  (<U0035>,<U0B6B>); /
  (<U0036>,<U0B6C>); /
  (<U0037>,<U0B6D>); /
  (<U0038>,<U0B6E>); /
  (<U0039>,<U0B6F>); 

translit_start
include  "translit_combining";""
translit_end
END LC_CTYPE

%%%%%%%%%%%%%
