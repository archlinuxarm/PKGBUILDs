END LC_COLLATE

LC_MESSAGES
% Yes=Eny --> EeYy as possible Yes chars
% No=Tsia -->TtNn
yesexpr "<U005E><U005B><U0065><U0045><U0079><U0059><U005D><U002E><U002A>"
noexpr  "<U005E><U005B><U0074><U0054><U006E><U004E><U005D><U002E><U002A>"
END LC_MESSAGES

