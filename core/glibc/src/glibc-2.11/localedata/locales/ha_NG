% no: Babu, A'a
nostr       "<U0042><U0061><U0062><U0075>"
% ^[BbAaNn].*
noexpr      "<U005E><U005B><U0062><U0042><U0061><U0041><U006E><U004E><U005D><U002E><U002A>"
