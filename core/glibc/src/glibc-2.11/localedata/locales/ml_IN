category     "ml_IN:2000";LC_IDENTIFICATION
category     "ml_IN:2000";LC_CTYPE
category     "ml_IN:2000";LC_COLLATE
category     "ml_IN:2000";LC_TIME
category     "ml_IN:2000";LC_NUMERIC
