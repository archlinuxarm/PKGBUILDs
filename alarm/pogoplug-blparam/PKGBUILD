pkgname=pogoplug-blparam
pkgver=1.0
pkgrel=4
pkgdesc="Blparam pulled from Pogoplug OS"
arch=('arm')
url="http://www.pogoplug.com"
license=('GPL')
source=('blparam')
md5sums=('df55bfb572b5c28927e7fae04c391656')

package() {
  mkdir -p ${pkgdir}/usr/bin
  install -m755 $srcdir/blparam ${pkgdir}/usr/bin/
}
