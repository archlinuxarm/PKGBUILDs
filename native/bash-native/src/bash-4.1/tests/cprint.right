        fi;
    fi > /dev/null;
    case $PATH in 
        *$PWD*)
            echo \$PWD in \$PATH
        ;;
        *)
            echo \$PWD not in \$PATH
        ;;
    esac > /dev/null;
    while false; do
        echo z;
    done > /dev/null;
    until true; do
        echo z;
    done > /dev/null;
    echo \&\|'()' \{ echo abcde \; \};
    eval fu\%nc'()' \{ echo abcde \; \};
    type fu\%nc
}
