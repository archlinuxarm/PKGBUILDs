    while (( i < 3 )); do
        test -r /dev/fd/$i;
        i=$(( i + 1 ));
    done;
    [[ -r /dev/fd/0 && -w /dev/fd/1 ]] || echo oops > /dev/null;
    for name in $( echo 1 2 3 );
    do
        test -r /dev/fd/$name;
    done;
    if [[ -r /dev/fd/0 && -w /dev/fd/1 ]]; then
        echo ok > /dev/null;
    else
        if (( 7 > 40 )); then
            echo oops;
