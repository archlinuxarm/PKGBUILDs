export ENV_SET="YES"		# avoid repeat

# File creation mask
umask 022			# all files created are -rw-r--r--

##
# Load user environment
##

if [ -f ${bash_initdir}/environment.mine ]; then
    source ${bash_initdir}/environment.mine
fi
