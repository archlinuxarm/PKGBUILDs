pkgname=wine_gecko
pkgver=1.0.0
pkgrel=1
pkgdesc="Wine's built-in replacement for Microsoft's Internet Explorer"
arch=('i686')
url="http://www.winehq.org/"
license=('MPL')
depends=('wine>=1.1.27')
source=(http://downloads.sourceforge.net/project/wine/Wine%20Gecko/${pkgver}/${pkgname}-${pkgver}-x86.cab)
md5sums=('9c5c335fc077c0558561afaf25a09e51')

build() {
    install -Dm644 ${srcdir}/${pkgname}-${pkgver}-x86.cab \
      ${pkgdir}/usr/share/wine/gecko/${pkgname}-${pkgver}-x86.cab
}
