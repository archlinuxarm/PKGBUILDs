# Maintainer: Geoffroy Carrier <geoffroy.carrier@@aur.archlinux.org>
# Contributor: Pat Rondon <pmr@@patrondon.org>
pkgname=urlgrabber
pkgver=3.1.0
pkgrel=2
pkgdesc="A high-level cross-protocol url-grabber and Python library"
url="http://linux.duke.edu/projects/urlgrabber/"
license=('LGPL')
arch=('i686' 'x86_64')
depends=('python')
source=(http://linux.duke.edu/projects/urlgrabber/download/urlgrabber-$pkgver.tar.gz)
build() {
      cd $srcdir/$pkgname-$pkgver
      python setup.py install --prefix=$pkgdir/usr || return 1
    }
md5sums=('2a92d8ce0d89c5e772a98e9b8dcd5b73')
