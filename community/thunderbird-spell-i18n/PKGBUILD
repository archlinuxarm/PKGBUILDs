package_thunderbird-spell-ru-ie() {
  pkgdesc="Russian (with £ [ie]) spellchecker files for Thunderbird"
  cd ${srcdir}
  bsdtar xf spell-ru-ie.xpi || return 1
  install -D -m644 ru.aff "${pkgdir}/usr/lib/thunderbird-2.0/dictionaries/ru-ie-RU.aff" || return 1
  install -D -m644 ru.dic "${pkgdir}/usr/lib/thunderbird-2.0/dictionaries/ru-ie-RU.dic" || return 1
}

