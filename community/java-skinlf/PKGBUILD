# Contributor: Simon Lipp <sloonz+aur@gmail.com>

pkgname=java-skinlf
pkgver=6.7
pkgrel=2
pkgdesc="Skinning engine for Swing"
arch=(i686 x86_64)
url="http://l2fprod.com/skinlf/"
license=(APACHE)
depends=('java-runtime')
makedepends=('')
source=('https://skinlf.dev.java.net/files/documents/66/37801/skinlf-6.7-20060722.zip')
md5sums=('09b41b511b51e465a052483b598d68f3')

build() {
  install -d $startdir/pkg/usr/share/java/skinlf/
  install --mode=644 $startdir/src/skinlf-6.7/lib/skinlf.jar $startdir/pkg/usr/share/java/skinlf/ && \
  install --mode=644 $startdir/src/skinlf-6.7/lib/nativeskin.jar $startdir/pkg/usr/share/java/skinlf/
}
